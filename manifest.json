{
  "manifest_version": 3,
  "name": "Hours to Days",
  "description": "A simple Chrome Extension created using a template",
  "version": "0.0.1",
  "author": "Clyde D'Souza",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "scripting",
    "activeTab"
  ]
}